        <service folder="Employees.services" name="getEmployee">
            <input>
                <file filename="resources/test/xml/data/GetEmployeeByIdInput.xml"/>
            </input>
            <expected>
                <file filename="resources/test/xml/data/GetEmployeeByIdOutput.xml"/>
            </expected>
        </service>
    </webMethodsTestCase>
</webMethodsTestSuite>
