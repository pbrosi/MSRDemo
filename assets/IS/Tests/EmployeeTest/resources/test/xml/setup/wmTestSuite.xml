<?xml version="1.0" encoding="UTF-8"?>
<webMethodsTestSuite description="" name="EmployeeTest">
    <webMethodsTestCase name="Get Employee">
        <service folder="Employees.services" name="getEmployee">
            <input>
                <file/>
            </input>
            <expected>
                <file filename="resources/test/xml/data/GetEmployees.xml"/>
            </expected>
        </service>
    </webMethodsTestCase>
